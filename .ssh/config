Host aur.archlinux.org
	IdentityFile ~/.ssh/aur
	User aur
	IdentitiesOnly yes

AddKeysToAgent yes
Host github.com
	HostName github.com
	ForwardAgent yes
	IdentityFile ~/.ssh/git
	User git
	IdentitiesOnly yes

